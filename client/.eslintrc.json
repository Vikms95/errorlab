{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "eslint-config-prettier",
        "@typescript-eslint/parser"
    ],
    "overrides": [
      {
        "files": ["*.jsx", "*.tsx"],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": ["off"]
        }
      }
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "/home/victor/Desktop/Projects/errorlab/client/tsconfig.json"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-unused-vars": "warn",
      "no-undef": "warn",
      "@typescript-eslint/no-unused-vars": ["warn"]
    }
}
